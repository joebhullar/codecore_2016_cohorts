<h1>My First Form in Sinatra</h1>
<form action="/contact" method="POST">
    <div>
        <label for="">First Name:</label>
        <input type="text" name='first_name' value="<%= Faker::Pokemon.name%>">
    </div>
    <div>
        <label for="">Last Name:</label>
        <input type="text" name='last_name' value="<%= Faker::Pokemon.name%>">
    </div>
    <input type="submit" value='Send it back'>
</form>

<div>
    <%= @last_name %>
    <br>
    <%= @first_name %>
</div>

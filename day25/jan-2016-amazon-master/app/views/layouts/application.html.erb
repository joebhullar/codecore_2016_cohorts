<!DOCTYPE html>
<html>
<head>
  <title>Amazon</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<%#= flash[:notice] || flash[:alert] %>

<% if session[:user_id] %>
  <%= link_to "log out", sessions_path, method: :delete %>
<% end %>

<%= notice || alert %>
<%= yield %>

</body>
</html>
